Function changeMoling
    Rem reStartFind
    Delay 800
    Dim isNeedChangeStar = 0
    //拿到用户的设置信息
    Dim star1 = ReadUIConfig("1星狗粮",false)
    Dim star2 = ReadUIConfig("2星狗粮",false)
    Dim star3 = ReadUIConfig("3星狗粮",false)
    Dim star4 = ReadUIConfig("4星狗粮",false)

    If star1 = True and star1Finished = False Then 
        isNeedChangeStar = 1
    Else 
        If star2 = True and star2Finished = False Then 
            isNeedChangeStar = 2
        Else 
            If star3 = True and star3Finished = False Then 
                isNeedChangeStar = 3
            Else 
                If star4 = True and star4Finished = False Then 
                    isNeedChangeStar = 4
                Else 
                    //1-4星全部练完,停止脚本
                    //这里狗粮已经带完一轮了，可能中途掉了2星，3星，继续练
                    If 狗粮一轮完毕 = False Then 
                        狗粮一轮完毕 = True
                        If star1 = True Then 
                            star1Finished = False
                        End If
                        If star2 = True Then 
                            star2Finished = False
                        End If
                        If star3 = True Then 
                            star3Finished = False
                        End If
                        Goto reStartFind
                    Else 
                        endReason = "狗粮已练完,停止脚本"
                        stopTheScript
                    End If
                End If
            End If
        End If
    End If
    TracePrint isNeedChangeStar & "星狗粮开始"
    //先将魔灵快速滑到最右侧
    If isScrollEnd = False Then 
        For 10
            TouchDown 230, 800, 1//按住屏幕上的100,100坐标不放，并设置此触点ID=1
            TouchMove 230, 300, 1, 200//将ID=1的触点花200毫秒移动至300,300坐标
            TouchUp 1//松开弹起ID=1的触点
            Delay 100
        Next
        //取两点颜色 
        Dim p1 = 1,p2 = 2,n1 = 11, n2 = 22
        Do Until n1 = p1 and n2 = p2
            Delay 500
            p1 = GetPixelColor(230,715)
            p2 = GetPixelColor(230,825)
            //        zm.Swipe 230, 800, 230, 300,50	//右滑
            Swipe 230, 800, 230, 300,1
            Delay 2000
            n1 = GetPixelColor(230,715)
            n2 = GetPixelColor(230, 825)
        Loop
        TracePrint "已滑到最右"  
        isScrollEnd = True
    End If 
    Rem startFind
    Delay 800
    //从最左到最右中寻找对应星级 230,10,295,900
    TracePrint "当前查找范围y:" & 狗粮寻找y
    While 是否存在狗粮星级(isNeedChangeStar,230,狗粮寻找y,295,900) = False
        Delay 800
        //范围内没有该星级,需要滑动再找
        p1 = GetPixelColor(230,715)
        p2 = GetPixelColor(230,825)
        zm.Swipe 230, 100, 230, 770, 500	//右滑
        Delay 1000
        n1 = GetPixelColor(230,715)
        n2 = GetPixelColor(230,825)
        //滑完后判断是否存在当前星级和高一星级
        Dim biggerStar = isNeedChangeStar + 1
        If 是否存在狗粮星级(isNeedChangeStar, 230, 狗粮寻找y, 295, 900) = False and 是否存在狗粮星级(biggerStar,230,狗粮寻找y,295,900) = True Then 
            TracePrint "没有找到当前星级，但是找到了高一级星级,停止当前循环"
            n1 = p1
            n2 = p2
        End If
        If n1 = p1 and n2 = p2 Then 
            TracePrint "已滑到最左"
            //所有魔灵均没有对应星级,设置为最大
            Select Case isNeedChangeStar
            Case 1
                star1Finished = True
            Case 2
                star2Finished = True
            Case 3
                star3Finished = True
            Case 4
                star4Finished = True
            Case Else
                TracePrint "魔灵星级异常"	
            End Select
            isScrollEnd = False
            狗粮寻找y = 10
            Goto reStartFind
        End If
    Wend
    TracePrint "找到符合星级魔灵：" & x & "," & y
    Dim x1 = x - 100, y1 = y, x2 = x, y2 = y + 100
    Dim tempP = x,tempQ = y
    //判断是不是满级了
    Dim isFullLevel = False
    Select Case isNeedChangeStar
    Case 1
        If customFindPic(x1, y1, x2, y2,"1") = True and customFindPic(x1, y1, x2, y2,"5") = True Then 
            TracePrint "1星魔灵已经满级"
            isFullLevel = True
        Else 
            TracePrint "1星魔灵未满级"
        End If	
    Case 2
        If customFindPic(x1, y1, x2, y2,"2") = True and customFindPic(x1, y1, x2, y2,"0") = True Then 
            TracePrint "2星魔灵已经满级"
            isFullLevel = True
        Else 
            TracePrint "2星魔灵未满级"
        End If
    Case 3
        If customFindPic(x1, y1, x2, y2,"2") = True and customFindPic(x1, y1, x2, y2,"5") = True Then 
            TracePrint "3星魔灵已经满级"
            isFullLevel = True
        Else 
            TracePrint "3星魔灵未满级"
        End If
    Case 4
        If customFindPic(x1, y1, x2, y2,"3") = True and customFindPic(x1, y1, x2, y2,"0") = True Then 
            TracePrint "4星魔灵已经满级"
            isFullLevel = True
        Else 
            TracePrint "4星魔灵未满级"
        End If
    Case Else
        TracePrint "魔灵星级异常"	
    End Select
    
    If y2 > 875 Then 
    	TracePrint "魔灵等级被遮挡，默认满级"
    	isFullLevel = True
    End If
    
    If isFullLevel = False and y2 < 875 Then 
        RandomClickInRectNoCheck tempP - 50, tempQ, 50, 50, 1
    Else 
        //找到星级了但是满级了,继续遍历,y坐标缩小
        If 狗粮寻找y + 50 > 875 Then 
            //范围已最小，需要滑动，还原Y值
            狗粮寻找y = 10
        Else 
            狗粮寻找y = 狗粮寻找y + 50
            Goto startFind
        End If
        p1 = GetPixelColor(230,715)
        p2 = GetPixelColor(230,825)
        zm.Swipe 230, 100, 230, 770, 500	//右滑
        //            maxY = 900	//复位
        //            minY = 850
        Delay 1000
        n1 = GetPixelColor(230,715)
        n2 = GetPixelColor(230,825)
        If n1 = p1 and n2 = p2 Then 
            TracePrint "已滑到最左"
            //所有魔灵均没有对应星级,设置为最大
            Select Case isNeedChangeStar
            Case 1
                star1Finished = True
            Case 2
                star2Finished = True
            Case 3
                star3Finished = True
            Case 4
                star4Finished = True
            Case Else
                TracePrint "魔灵星级异常"	
            End Select
            isScrollEnd = False
            狗粮寻找y = 10
            Goto reStartFind
        End If
        Goto startFind
    End If
End Function
