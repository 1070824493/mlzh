Function CustomFindMulColor(minX, minY, maxX, maxY, tb)
    //	TracePrint minx,minY,maxX,maxY
    Dim result = False     
    Dim i
    For Each v In tb
        //                TracePrint v[1], v[2] 
        dim arr = Split(v[2],",")
        //        TracePrint zm.VarInfo(arr) 
        If result = True Then 
