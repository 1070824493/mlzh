Function judgeMoLingStar(x1,y1,x2,y2,isTop)
    If isTop = True Then 
        If customFindPic(x1,y1,x2,y2,"黄色狗粮六星") = true or customFindPic(x1,y1,x2,y2,"红色狗粮六星") = true or customFindPic(x1,y1,x2,y2,"橙色狗粮六星") = true Then 
