Function 比例判断脚本模式
    TracePrint "开始判断脚本模式"
    Dim UIType = ReadUIConfig("模式",0)
    If UIType = 0 Then 
        //地下城模式
        type = 10
        TracePrint "地下城模式"
        ShowMessage "地下城模式"
        地下城循环	
    End If
    If UIType = 1 Then 
        type = 20
        TracePrint "异界地下城"
        ShowMessage "异界地下城"
        Delay 2000
        异界地下循环
    End If
    If UIType = 2 Then 
        type = 1
        TracePrint "爬塔模式"
        ShowMessage "爬塔模式"
        Delay 2000
        爬塔循环
    End If
    If UIType = 3 Then 
        type = 3
        TracePrint "异界突袭"
        ShowMessage "异界突袭"
        Delay 2000
        突袭模式
    End If
    If UIType = 4 Then 
        type = 4
        TracePrint "狗粮模式"
        ShowMessage "狗粮模式"
        Delay 2000
        狗粮循环
    End If
    If UIType = 5 Then 
        type = 5
        TracePrint "普通竞技场"
        ShowMessage "普通竞技场"
        Delay 2000
        竞技场循环
    End If
    //	
    If UIType = 6 Then 
        type = 6
        TracePrint "次元模式"
        ShowMessage "次元模式"
        Delay 2000
        地下城循环
    End If
    //    endReason = "识别模式失败,请在战斗准备界面开启脚本,如无误,则脚本不支持,目前还在优化中~"
