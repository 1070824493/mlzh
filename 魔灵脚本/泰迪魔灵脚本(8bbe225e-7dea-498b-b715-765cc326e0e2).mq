        TracePrint "出现了答题"
        Rem 重新识别题目
        Delay 800
        dim ret = zm.OcrBaiDu(483,422,546,852)
        If IsNull(ret) or ret = "" Then 
            ShowMessage "答题服务器异常"
            TracePrint "答题服务器异常"
            Delay 3000
            goto 重新识别题目
        Else 
            TracePrint "识别结果:" & ret
            ShowMessage "识别结果:" & ret
        End If
        falseTime = 0
        Rem question
        Delay 800
        //先截图答题
        Dim fileName = "/" & Replace(GetNetworkTime()," ","-")  &".png" 
        SnapShot dirPath & fileName
        TracePrint "截图路径：" & dirPath & fileName
