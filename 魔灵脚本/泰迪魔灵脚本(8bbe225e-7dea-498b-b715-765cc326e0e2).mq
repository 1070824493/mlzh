            Goto didFind
        End If
        Dim current = True
        For Each c In arr
            Dim intX,intY
            //                        TracePrint c
            FindColor minX, minY, maxX, maxY, c, 0, 1, intX, intY		
            If intX < 0 and intY < 0 Then 
                //有一个没找到就终止循环
                current = False 
